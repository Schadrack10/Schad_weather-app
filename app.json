{
  "expo": {
    "name": "Schad_Weather_pro",
    "slug": "weather-app",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/brand.png",
    "splash": {
      "image": "./assets/weather-logo.png",
      "resizeMode": "contain",
      "backgroundColor": "#000000"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#FFFFFF"
      },
      "package": "schadrack.weatherapp",
      "versionCode": 1,
      "permissions":["LOCATION"]
    },
    "web": {
      "favicon": "./assets/brand.png"
    }
  }
}
